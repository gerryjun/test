{
  "name": "hello-world-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://venerguevarra.github.io/hello-world-app/",
  "dependencies": {
    "create-react-class": "^15.6.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "set PORT=3000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "npm run cleanup && gh-pages -d build -b deploy -e build",
    "cleanup": "rm -rf node_modules/gh-pages/.cache"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0"
  }
}
